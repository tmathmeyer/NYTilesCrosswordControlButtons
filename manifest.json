{
  "manifest_version": 2,
  "name": "NY Crossword Next Button",
  "description": "Adds a next and previous button to the NYTimes Crossword pages",
  "browser_action": {
      "default_title": "NYTimes Next Button"
  },
  "version": "0.1",
  "content_scripts": [
      {
          "matches": [
              "https://www.nytimes.com/crosswords/*"
          ],
          "js": ["nyxwordnext.js"]
      }
  ]
}